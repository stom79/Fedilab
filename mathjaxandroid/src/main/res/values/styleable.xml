<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MathJaxView">
        <attr name="android:gravity" />
        <attr name="verticalScrollbarsEnabled" format="boolean" />
        <attr name="horizontalScrollbarsEnabled" format="boolean" />
        <attr name="textColor" format="string" />
        <attr name="input" format="enum">
            <enum name="TeX" value="0" />
            <enum name="MathML" value="1" />
            <enum name="AsciiMath" value="2" />
        </attr>
        <attr name="output" format="enum">
            <enum name="SVG" value="0" />
            <enum name="HTML_CSS" value="1" />
            <enum name="CommonHTML" value="2" />
            <enum name="NativeMML" value="3" />
        </attr>
        <attr name="outputScale" format="integer" />
        <attr name="minScaleAdjust" format="integer" />
        <attr name="automaticLinebreaks" format="boolean" />
        <attr name="blacker" format="integer" />
    </declare-styleable>

</resources>